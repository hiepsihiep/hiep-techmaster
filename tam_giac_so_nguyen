num = int(input("Nhap so tu 5 den 10: "))
while num < 5 or num > 10:
    if num < 5 or num > 10:
        print("Ngoai khoang 5 den 10!")
        num = int(input("Nhap so tu 5 den 10: "))
while num >= 5 or num <= 10:
    for i in range(1, num + 1):
        for j in range(1, i + 1):
            print(j, end="")
        print("")
    num = int(input("Nhap so tu 5 den 10: "))
